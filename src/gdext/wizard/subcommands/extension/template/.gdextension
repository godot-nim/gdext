[configuration]

entry_symbol = "init_library"
compatibility_minimum = 4.3
reloadable = true

[libraries]

linux.debug = "res://nim/$name/lib/linux/debug/libbootstrap.so"
windows.debug = "res://nim/$name/lib/windows/debug/bootstrap.dll"
macos.debug = "res://./nim/$name/lib/macos/debug/libbootstrap.dylib"

linux.release = "res://nim/$name/lib/linux/release/libbootstrap.so"
windows.release = "res://nim/$name/lib/windows/release/bootstrap.dll"
macos.release = "res://./nim/$name/lib/macos/release/libbootstrap.dylib"